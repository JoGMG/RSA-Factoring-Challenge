#!/usr/bin/python3
from sys import argv

def factorize(num):
	f1 = 2
	if num < 2:
		return
	
	while (num % f1):
		f1 += 1
	f2 = num // f1
	print("{}={}*{}".format(num, f2, f1))

if len(argv) != 2:
	exit()

try:
	with open(argv[1]) as file:
		line = file.readline()

		while line != "":
			num = int(line.split('\n')[0])
			factorize(num)
			line =  file.readline()
except:
	pass
